      <section name="ajaxSettings" type="AjaxPro.AjaxSettingsSectionHandler,AjaxPro.2"/>
    </sectionGroup>
    <section name="RewriterConfig" type="URLRewriter.RewriterConfigSerializerSectionHandler"/>
